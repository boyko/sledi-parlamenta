<h1>Members (<%= @members.count %>)</h1>

<table border=1>
  <tr>
    <td>index</td>
    <td>Имена</td>
    <td>GOV ID</td>
  </tr>
<% @members.each_with_index do |m, index| %>
  <tr>
    <td><%= index %></td>
    <td><%= link_to m.names, member_path(m) %></td>
    <td><%= m.gov_site_id %></td>
  </tr>
<% end %>

</table>
